{
  "name": "husky-conflict-hook",
  "version": "0.0.3",
  "description": "husky hook used to check for git conflict",
  "main": "./bin/index.js",
  "bin": {
    "husky-conflict-hook": "bin/index.js"
  },
  "scripts": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "node ./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Ka-Houl/husky-conflict-hook.git"
  },
  "files": [
    "bin"
  ],
  "keywords": [
    "husky",
    "hooks",
    "pre-commit",
    "lint-staged",
    "conflict",
    "git",
    "git-commit",
    "git-conflict"
  ],
  "author": "Ka-Houl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ka-Houl/husky-conflict-hook/issues"
  },
  "homepage": "https://github.com/Ka-Houl/husky-conflict-hook#readme",
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^11.0.0"
  }
}
